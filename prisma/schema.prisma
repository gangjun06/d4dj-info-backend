generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id            Int      @id @default(autoincrement())
  email         String   @unique
  name          String
  password      String
  isAdmin       Boolean  @default(false)
  emailVerified Boolean  @default(false)
  createAt      DateTime @default(now())
}
